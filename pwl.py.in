## pylint: disable=invalid-name
## XXX pylint: disable=missing-docstring

from pylisp import lisp

__all__ = ("lisp", "load")


preload_src = r"""
;; @preload_src@
"""


def load():
    lisp.execute(preload_src)


if __name__ == "__main__":
    load()
    lisp.main()


